{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"jsx": "react",
		"lib": ["es5", "es6", "dom"],
		"strictNullChecks": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"skipLibCheck": true
	},
	"exclude": [
		"dist/",
		"node_modules/"
	]
}
