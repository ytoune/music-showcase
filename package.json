{
  "name": "music-showcase",
  "version": "2.5.1",
  "description": "",
  "author": {
    "name": "ytoune"
  },
  "engines": {
    "node": ">= 16"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "deploy": "yarn build && yarn push-page",
    "push-page": "gh-pages --dist out --dotfiles",
    "build": "rimraf out .next && next build && next export && touch out/.nojekyll",
    "dev": "next",
    "ts-node": "ts-node --files -O '{\"module\":\"commonjs\"}'",
    "test": "eslint '**/*.{ts,tsx}' && tsc --noEmit"
  },
  "dependencies": {
    "preact": "10.6.3",
    "preact-render-to-string": "5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.9.2",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.8.1",
    "@mui/material": "5.8.1",
    "@types/encoding-japanese": "2.0.0",
    "@types/gh-pages": "3.2.1",
    "@types/jest": "27.5.1",
    "@types/mime-types": "2.1.1",
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "encoding-japanese": "2.0.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "gh-pages": "4.0.0",
    "jest": "28.1.0",
    "jszip": "3.10.0",
    "mime-types": "2.1.35",
    "music-metadata-browser": "2.5.6",
    "next": "12.1.6",
    "next-plugin-preact": "3.0.6",
    "node-libs-browser": "2.2.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "scheduler": "0.22.0",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "util": "0.12.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    }
  },
  "browser": {
    "fs": false,
    "child_process": false
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": [
          "*.md",
          "*.yml",
          "*.json",
          "*.yaml"
        ],
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
